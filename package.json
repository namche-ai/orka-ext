{
	"name": "orka-dev",
	"displayName": "ORKA",
	"description": "Autonomous coding agent right in your IDE, capable of creating/editing files, running commands, using the browser, and more with your permission every step of the way.",
	"version": "3.16.1",
	"icon": "assets/icons/icon.png",
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "Orka Bot Inc."
	},
	"license": "Apache-2.0",
	"publisher": "saoudrizwan",
	"repository": {
		"type": "git",
		"url": "https://github.com/namche-ai/orka-ext"
	},
	"homepage": "https://orka.dev",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"orka",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished",
		"workspaceContains:evals.env"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "orka-dev-ActivityBar",
					"title": "ORKA",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"orka-dev-ActivityBar": [
				{
					"type": "webview",
					"id": "orka-dev.SidebarProvider",
					"name": "ORKA"
				}
			]
		},
		"commands": [
			{
				"command": "orka.plusButtonClicked",
				"title": "新的任务",
				"icon": "$(add)"
			},
			{
				"command": "orka.mcpButtonClicked",
				"title": "MCP 服务器",
				"icon": "$(server)"
			},
			{
				"command": "orka.historyButtonClicked",
				"title": "历史记录",
				"icon": "$(history)"
			},
			{
				"command": "orka.popoutButtonClicked",
				"title": "在编辑器中打开",
				"icon": "$(link-external)"
			},
			{
				"command": "orka.accountButtonClicked",
				"title": "账户",
				"icon": "$(account)"
			},
			{
				"command": "orka.settingsButtonClicked",
				"title": "设置",
				"icon": "$(settings-gear)"
			},
			{
				"command": "orka.openInNewTab",
				"title": "在新标签中打开",
				"category": "Orka"
			},
			{
				"command": "orka.dev.createTestTasks",
				"title": "创建测试任务",
				"category": "Orka",
				"when": "orka.isDevMode"
			},
			{
				"command": "orka.addToChat",
				"title": "添加到ORKA",
				"category": "Orka"
			},
			{
				"command": "orka.addTerminalOutputToChat",
				"title": "添加到ORKA",
				"category": "Orka"
			},
			{
				"command": "orka.focusChatInput",
				"title": "跳到聊天输入框",
				"category": "Orka"
			},
			{
				"command": "orka.generateGitCommitMessage",
				"title": "使用ORKA生成提交信息",
				"category": "Orka",
				"icon": "$(robot)"
			}
		],
		"keybindings": [
			{
				"command": "orka.addToChat",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "editorHasSelection"
			},
			{
				"command": "orka.generateGitCommitMessage",
				"when": "scmProvider == git"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "orka.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == orka-dev.SidebarProvider"
				},
				{
					"command": "orka.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == orka-dev.SidebarProvider"
				},
				{
					"command": "orka.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == orka-dev.SidebarProvider"
				},
				{
					"command": "orka.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == orka-dev.SidebarProvider"
				},
				{
					"command": "orka.accountButtonClicked",
					"group": "navigation@5",
					"when": "view == orka-dev.SidebarProvider"
				},
				{
					"command": "orka.settingsButtonClicked",
					"group": "navigation@6",
					"when": "view == orka-dev.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "orka.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == orka-dev.TabPanelProvider"
				},
				{
					"command": "orka.mcpButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == orka-dev.TabPanelProvider"
				},
				{
					"command": "orka.historyButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == orka-dev.TabPanelProvider"
				},
				{
					"command": "orka.popoutButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == orka-dev.TabPanelProvider"
				},
				{
					"command": "orka.accountButtonClicked",
					"group": "navigation@5",
					"when": "activeWebviewPanelId == orka-dev.TabPanelProvider"
				},
				{
					"command": "orka.settingsButtonClicked",
					"group": "navigation@6",
					"when": "activeWebviewPanelId == orka-dev.TabPanelProvider"
				}
			],
			"editor/context": [
				{
					"command": "orka.addToChat",
					"group": "navigation",
					"when": "editorHasSelection"
				}
			],
			"terminal/context": [
				{
					"command": "orka.addTerminalOutputToChat",
					"group": "navigation"
				}
			],
			"scm/title": [
				{
					"command": "orka.generateGitCommitMessage",
					"group": "navigation",
					"when": "scmProvider == git"
				}
			],
			"commandPalette": [
				{
					"command": "orka.generateGitCommitMessage",
					"when": "scmProvider == git"
				}
			]
		},
		"configuration": {
			"title": "ORKA",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "cnpm run package",
		"compile": "cnpm run check-types && cnpm run lint && node esbuild.js",
		"compile-standalone": "cnpm run protos && cnpm run check-types && cnpm run lint && node esbuild.js --standalone",
		"postcompile-standalone": "node scripts/package-standalone.mjs",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "cnpm run build:webview && cnpm run check-types && cnpm run lint && node esbuild.js --production",
		"protos": "node proto/build-proto.js && node scripts/generate-server-setup.mjs",
		"postprotos": "prettier src/shared/proto src/core/controller webview-ui/src/services src/standalone/server-setup.ts --write --log-level silent",
		"compile-tests": "node ./scripts/build-tests.js",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "cnpm run compile-tests && cnpm run compile && cnpm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts && eslint webview-ui/src --ext ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"test": "npm-run-all test:unit test:integration",
		"test:ci": "node scripts/test-ci.js",
		"test:integration": "vscode-test",
		"test:unit": "TS_NODE_PROJECT='./tsconfig.unit-test.json' mocha",
		"test:coverage": "vscode-test --coverage",
		"install:all": "cnpm install && cd webview-ui && cnpm install",
		"dev:webview": "cd webview-ui && cnpm run dev",
		"build:webview": "cd webview-ui && cnpm run build",
		"test:webview": "cd webview-ui && cnpm run test",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"docs": "cd docs && mintlify dev",
		"docs:check-links": "cd docs && mintlify broken-links",
		"docs:rename-file": "cd docs && mintlify rename",
		"report-issue": "node scripts/report-issue.js"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.12",
		"@types/chai": "^5.0.1",
		"@types/clone-deep": "^4.0.4",
		"@types/diff": "^5.2.1",
		"@types/get-folder-size": "^3.0.4",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/pdf-parse": "^1.1.4",
		"@types/proxyquire": "^1.3.31",
		"@types/should": "^11.2.0",
		"@types/sinon": "^17.0.4",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.84.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"chai": "^4.3.10",
		"chalk": "^5.3.0",
		"esbuild": "^0.25.0",
		"eslint": "^8.57.0",
		"grpc-tools": "^1.13.0",
		"husky": "^9.1.7",
		"mintlify": "^4.0.515",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"protoc-gen-ts": "^0.8.7",
		"proxyquire": "^2.1.3",
		"should": "^13.2.3",
		"sinon": "^19.0.2",
		"ts-node": "^10.9.2",
		"ts-proto": "^2.6.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.12.4",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.6.4",
		"@aws-sdk/client-bedrock-runtime": "^3.758.0",
		"@bufbuild/protobuf": "^2.2.5",
		"@google-cloud/vertexai": "^1.9.3",
		"@google/genai": "^0.13.0",
		"@grpc/grpc-js": "^1.9.15",
		"@grpc/reflection": "^1.0.4",
		"@mistralai/mistralai": "^1.5.0",
		"@modelcontextprotocol/sdk": "^1.7.0",
		"@opentelemetry/api": "^1.4.1",
		"@opentelemetry/exporter-trace-otlp-http": "^0.39.1",
		"@opentelemetry/resources": "^1.30.1",
		"@opentelemetry/sdk-node": "^0.39.1",
		"@opentelemetry/sdk-trace-node": "^1.30.1",
		"@opentelemetry/semantic-conventions": "^1.30.0",
		"@sentry/browser": "^9.12.0",
		"@vscode/codicons": "^0.0.36",
		"archiver": "^7.0.1",
		"axios": "^1.8.2",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"chrome-launcher": "^1.1.2",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"diff": "^5.2.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"firebase": "^11.2.0",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"grpc-health-check": "^2.0.2",
		"iconv-lite": "^0.6.3",
		"ignore": "^7.0.3",
		"image-size": "^2.0.2",
		"isbinaryfile": "^5.0.2",
		"jschardet": "^3.1.4",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"ollama": "^0.5.13",
		"open": "^10.1.2",
		"open-graph-scraper": "^6.9.0",
		"openai": "^4.83.0",
		"os-name": "^6.0.0",
		"p-timeout": "^6.1.4",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"posthog-node": "^4.8.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"strip-ansi": "^7.1.0",
		"tree-sitter-wasms": "^0.1.11",
		"ts-morph": "^25.0.1",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.24.2"
	}
}
